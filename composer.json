{
    "name": "noahlvb/valueobject-bundle",
    "description": "Symfony integration for my value object package",
    "keywords": [
        "valueObjects",
        "DDD",
        "scalar type",
        "values",
        "OOP",
        "Symfony",
        "Forms",
        "Doctrine"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Noah van Bochove",
            "email": "mail@noahlvb.nl"
        }
    ],
    "autoload": {
        "psr-4": {
            "Noahlvb\\ValueObjectBundle\\": ["src/"]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Noahlvb\\ValueObjectBundle\\Tests\\": ["tests/"]
        }
    },
    "require": {
        "php": "^8.0",
        "noahlvb/valueobject": "^1.0",
        "doctrine/dbal": "^2.10||^3.0",
        "symfony/form": "^6.0||^7.0",
        "symfony/validator": "^6.0||^7.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "squizlabs/php_codesniffer": "^3.5",
        "phpunit/phpunit": "^9.0"
    }
}
